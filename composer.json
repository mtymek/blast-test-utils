{
    "name": "mtymek/blast-test-utils",
    "description": "Utilities for testing integrity of services managed by Zend\\ServiceManager",
    "license": "BSD-2-Clause",
    "authors": [
        {
            "name": "Mateusz Tymek",
            "email": "mtymek@gmail.com"
        }
    ],
    "require": {
        "zendframework/zend-servicemanager": "^3.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.4",
        "squizlabs/php_codesniffer": "^2.6"
    },
    "autoload": {
        "psr-4": {
            "Blast\\TestUtils\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Blast\\Test\\TestUtils\\": "test"
        }
    },
    "scripts": {
        "check": [
            "@cs",
            "@test"
        ],
        "cs": "phpcs",
        "cs-fix": "phpcbf",
        "test": "phpunit"
    }
}
